package threading;

public class JoinExample2 {

	public void run() {
		for(int i = 1; i <= 4; i++) {
			try {
				Thread.sleep(1000);
			} catch(Exception ex) {
				ex.printStackTrace();
			}
			System.out.println(i);
		}
	}
	
	public static void main(String args[]) {
		
		JoinExample2 t1 = new JoinExample2()
		
	}
	
}
